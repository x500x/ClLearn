    <EnumValue Name="none" DisplayName="Nigdy nie wysyłaj" />
    <EnumValue Name="prompt" DisplayName="Monituj przed wysłaniem" />
    <EnumValue Name="queue" DisplayName="Kolejka" />
    <EnumValue Name="send" DisplayName="Wyślij" />
