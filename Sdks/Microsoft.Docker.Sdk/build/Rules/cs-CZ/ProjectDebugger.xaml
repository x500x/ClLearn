<Rule Name="ProjectDebugger" DisplayName="Docker Compose" PageTemplate="Debugger" Description="Možnosti ladění Dockeru" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns="http://schemas.microsoft.com/build/2009/properties">

    <Rule.DataSource>
        <DataSource Persistence="UserFileWithXamlDefaults" HasConfigurationCondition="False"/>
    </Rule.DataSource>

    <!-- the command which appears in the debugger dropdown -->
    <Rule.Metadata>
        <sys:Guid x:Key="DebugTargetTypeCommandGuid">568ABDF7-D522-474D-9EED-34B5E5095BA5</sys:Guid>
        <sys:UInt32 x:Key="DebugTargetTypeCommandId">0x100</sys:UInt32>
        <sys:String x:Key="DebugTargetDropdownEnum">ActiveDebugProfile</sys:String>
    </Rule.Metadata>

    <DynamicEnumProperty Name="ActiveDebugProfile" DisplayName="Profil ladění" EnumProvider="DockerDebugProfileProvider" Description="Určuje profil používaný k ladění"/>
</Rule>
