<!-- Rule definition for project state in property sheet -->
<Rule Name="ConfigurationGeneralBrowseObject" DisplayName="Propiedades del proyecto" PageTemplate="generic" Description="Propiedades del proyecto" xmlns="http://schemas.microsoft.com/build/2009/properties">
    <Rule.Categories>
        <Category Name="Misc" DisplayName="Varios" Description="Varios"/>
        <Category Name="Compose" DisplayName="Docker Compose" Description="Docker Compose"/>
    </Rule.Categories>
    <Rule.DataSource>
        <DataSource Persistence="ProjectFile" Label="Configuration" HasConfigurationCondition="false"/>
    </Rule.DataSource>

    <StringProperty Name="ProjectFile" ReadOnly="True" Category="Misc" DisplayName="Archivo de proyecto" Description="Nombre del archivo del proyecto.">
        <StringProperty.DataSource>
            <DataSource Persistence="ProjectFile" PersistedName="MSBuildProjectFile"/>
        </StringProperty.DataSource>
    </StringProperty>

    <StringProperty Name="ProjectFolder" ReadOnly="True" Category="Misc" DisplayName="Carpeta de proyecto" Description="Ubicaci贸n del archivo de proyecto.">
        <StringProperty.DataSource>
            <DataSource Persistence="ProjectFile" PersistedName="MSBuildProjectDirectory"/>
        </StringProperty.DataSource>
    </StringProperty>

    <StringProperty Name="FullPath" ReadOnly="true" Visible="false">
        <StringProperty.DataSource>
            <DataSource Persistence="ProjectFile" PersistedName="MSBuildProjectFullPath"/>
        </StringProperty.DataSource>
    </StringProperty>

    <DynamicEnumProperty Name="LauchProfile" Category="Compose" DisplayName="Perfil de depuraci贸n activo" EnumProvider="DockerDebugProfileProvider" Description="Se us贸 un perfil de inicio activo durante la depuraci贸n.">
        <DynamicEnumProperty.DataSource>
            <DataSource Persistence="UserFile" PersistedName="ActiveDebugProfile" HasConfigurationCondition="False"/>
        </DynamicEnumProperty.DataSource>
    </DynamicEnumProperty>
</Rule>
